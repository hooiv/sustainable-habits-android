<!--
  HabitFlow launcher foreground icon.
  Design: a rounded leaf (sustainability) with an inset checkmark arc (habit completion).
  Safe zone: 72×72 centred in the 108×108 canvas (18dp inset on all sides).
-->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- Outer leaf shape — soft rounded teardrop pointing upward -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="
          M54,20
          C54,20 26,38 26,60
          C26,75.464 38.536,88 54,88
          C69.464,88 82,75.464 82,60
          C82,38 54,20 54,20 Z" />

    <!-- Inner leaf vein highlight for depth -->
    <path
        android:fillColor="#00000000"
        android:strokeColor="#CCFFFFFF"
        android:strokeWidth="2"
        android:pathData="M54,30 Q54,58 54,82" />

    <!-- Checkmark drawn as a thick stroke arc within the leaf -->
    <path
        android:fillColor="#00000000"
        android:strokeColor="#4DB6AC"
        android:strokeWidth="5.5"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:pathData="M38,60 L49,71 L70,50" />
</vector>