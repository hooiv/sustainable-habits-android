<?xml version="1.0" encoding="utf-8"?>
<!--
    Backup rules for Android 11 and below.
    Sensitive encrypted prefs are excluded to prevent leaking user credentials in backups.
    See: https://developer.android.com/guide/topics/data/autobackup
-->
<full-backup-content>
    <exclude domain="sharedpref" path="onboarding.xml" />
    <exclude domain="sharedpref" path="user_session.xml" />
</full-backup-content>
