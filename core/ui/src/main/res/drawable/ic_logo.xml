<!--
  HabitFlow in-app logo.
  A white leaf (sustainability) with a teal checkmark (habit completion).
  Designed for use on a solid or gradient background.
  ViewBox: 0 0 96 96
-->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="96dp"
    android:height="96dp"
    android:viewportWidth="96"
    android:viewportHeight="96">

    <!-- Circular background -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M48,4 A44,44 0 1 1 48,92 A44,44 0 1 1 48,4 Z" />

    <!-- Leaf body â€” rounded teardrop pointing up, filled with brand indigo -->
    <path
        android:fillColor="#3D3A8C"
        android:pathData="
          M48,16
          C48,16 24,32 24,52
          C24,65.255 34.745,76 48,76
          C61.255,76 72,65.255 72,52
          C72,32 48,16 48,16 Z" />

    <!-- Leaf centre vein -->
    <path
        android:fillColor="#00000000"
        android:strokeColor="#5A57B0"
        android:strokeWidth="1.5"
        android:pathData="M48,24 Q48,52 48,70" />

    <!-- Checkmark in teal -->
    <path
        android:fillColor="#00000000"
        android:strokeColor="#00B8A9"
        android:strokeWidth="5"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:pathData="M33,52 L43,63 L63,43" />

</vector>
